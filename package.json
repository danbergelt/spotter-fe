{
  "name": "spotter-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^6.10.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "codecov": "^3.6.5",
    "formik": "^2.1.4",
    "husky": "^4.0.10",
    "immer": "^5.3.6",
    "lint-staged": "^10.0.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.10.2",
    "react-helmet-async": "^1.0.4",
    "react-icons": "^3.8.0",
    "react-loader-spinner": "^3.1.5",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scrolllock": "^5.0.0",
    "react-textarea-autosize": "^7.1.2",
    "react-tiny-popover": "^4.0.0",
    "react-use": "^13.14.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.4",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test -i",
    "test:cov": "npm test -- --coverage --watchAll=false && codecov",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "fix": "eslint --fix . --ext .js,.ts,.ts,.tsx src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!/node_modules/",
      "!src/__tests__/",
      "!src/__testUtils__/",
      "!src/__mocks__/"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "yarn fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^8.0.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.17",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-loader-spinner": "^3.1.0",
    "@types/react-modal": "^3.10.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-textarea-autosize": "^4.3.5",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "identity-obj-proxy": "^3.0.0",
    "mq-polyfill": "^1.1.8",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3"
  }
}
